{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"pages/*": ["src/pages/*"],
			"components/*": ["src/components/*"],
			"routes/*": ["src/routes/*"],
			"constants/*": ["src/constants/*"],
			"hooks/*": ["src/hooks/*"],
			"layouts/*": ["src/layouts/*"],
			"styles/*": ["src/styles/*"],
			"assets/*": ["src/assets/*"],
			"utils/*": ["src/utils/*"]
		},
		"target": "ESNext",
		"jsx": "preserve"
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
